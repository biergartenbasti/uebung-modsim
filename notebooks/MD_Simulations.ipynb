      "source": [
        "def plot_MD(Integratorin=1, Tempin=300, Gammain=13, Stepnumberin=100, Ekinin=25, anglein=45): # Ekinin ist in meV\n",
        "    def Potential(r):\n",
        "        [x, y] = tpia*r\n",
        "        Energy = 2. + np.cos(x) + np.cos(y) #Potenzial V, siehe Aufgabenblatt\n",
        "        Force  = np.array([np.sin(x), np.sin(y)]) #Gradient: -dEnergy/dx, -dEnergy/dy\n",
        "        return [V0*Energy, V0*tpia*Force]\n",
        "    \n",
        "    \n",
        "    def Verlet(r1, r0, dt):             # Verlet(Positionsa[:,i-1],Positionsa[:,i-2], dt)\n",
        "        Enerout, F = Potential(r1)  # Kraft bei Step (i-1)\n",
        "        nextpos = 2.*r1 - r0 + F/m*dt2\n",
        "        vel = 0.5*(nextpos-r0)/dt\n",
        "        Ekin= 0.5*m*np.sum(vel**2)  # Ekin am der der aktuellen Position (i-1), also nicht an nextpos (i)\n",
        "        return nextpos, Enerout, Ekin\n",
        "    \n",
        "    \n",
        "    def VelVerlet(r, v, dt):\n",
        "        F       = Potential(r)[1] # Kraft bei Step (i-1)\n",
        "        nextpos = r + v*dt + 0.5*F/m*dt2\n",
        "        Enerout, nextF   = Potential(nextpos)\n",
        "        nextvel = v + 0.5*(F+nextF)/m*dt\n",
        "        Ekin    = 0.5*m*np.sum(nextvel**2)\n",
        "        return nextpos, nextvel, Enerout, Ekin\n",
        "        \n",
        "    \n",
        "    def Langevin(r, v, dt, rand1, rand2):\n",
        "        F       = Potential(r)[1] # Kraft bei Step (i-1)\n",
        "        C1      = 0.5*(F/m-gamma*v)*dt2\n",
        "        C2      = sigma*(0.5*rand1+0.5/np.sqrt(3.)*rand2)*dt*dtsq\n",
        "    #    C2      = sigma*rand1*dt*dtsq\n",
        "        nextpos = r + v*dt + C1 + C2\n",
        "        Enerout, nextF   = Potential(nextpos)\n",
        "        C3      = -gamma*v*dt + sigma*rand1*dtsq\n",
        "        nextvel = v + 0.5*(F+nextF)/m*dt + C3 - gamma*(C1+C2)\n",
        "        Ekin    = 0.5*m*np.sum(nextvel**2)\n",
        "        return nextpos, nextvel, Enerout, Ekin\n",
        "    \n",
        "        return\n",
        "    \n",
        "    \n",
        "    def Initialize(Ekin, angle, Stepnumber, dt):\n",
        "        posstart = np.array([1.5, 1.5])*10**(-10)\n",
        "        ForceStart =  Potential(posstart)[1] # in erstes Listenelement der ForceStart-Liste wird Energie und Kraft eingetragen\n",
        "        vel = np.sqrt(2.*Ekin/m)        # Startgeschwindigkeit\n",
        "        vstart = vel*np.array([np.cos(angle), np.sin(angle)]) # Startgeschwindigkeit als Vektor\n",
        "        \n",
        "        posArray= np.zeros([2, Stepnumber]) # Nullmatrix der Größe '2 x Stepnumber' erstellen\n",
        "        Etota   = np.zeros(Stepnumber)       # Nullvektor der Größe 'Stepnumber' erstellen\n",
        "        Ekina   = np.zeros(Stepnumber)\n",
        "        R2   = np.zeros(Stepnumber)\n",
        "        R2D   = np.zeros(Stepnumber)        \n",
        "        posArray[:,0] = posstart\n",
        "    \n",
        "        Etota[0] = Potential(posstart)[0] + Ekin # Epot+Ekin\n",
        "        Ekina[0] = Ekin\n",
        "    \n",
        "        if Integrator==0 :\n",
        "            print(\"Integrator: Verlet\\n\")\n",
        "    \n",
        "            pos1 = posstart + dt*vstart + 0.5*ForceStart/m*dt2 # Simple Integration der Newtonschen Bewegungsgleichung\n",
        "            posArray[:,1] = pos1\n",
        "            E1 = Potential(pos1)[0] # Epot nach dem ersten Schritt\n",
        "            Etota[1] = Potential(posstart)[0] + Ekin # Energieerhaltung\n",
        "            Ekina[1] = Etota[1] - E1\n",
        "        elif Integrator==1 :\n",
        "            print(\"Integrator: Velocity Verlet\\n\")\n",
        "        elif Integrator==2 :\n",
        "            print(\"Integrator: Langevin\\n\")\n",
        "        elif Integrator==3 :\n",
        "            print(\"Integrator: Monte Carlo\")\n",
        "        else :\n",
        "            print(\"Integrator: Wrong!\\n\")\n",
        "            return False\n",
        "    \n",
        "        return posArray, Etota, Ekina, vstart, R2, R2D \n",
        "    \n",
        "    \n",
        "    \n",
        "    ##################################\n",
        "    #The real programme starts here\n",
        "    \n",
        "    eVtoJ = 1.6022*10**(-19) # 1eV in kg*m^(2)s^(-2)\n",
        "    V0    = 0.025 * eVtoJ \n",
        "    tpia  = 2.*np.pi/3.*10**(10)    # 2pi/a a=Gitterkonstante in m (=3A)\n",
        "    m     = 26.98*1.66*10**(-27)        # Masse Al-Atom in kg\n",
        "    kB = 1.380649*10**(-23) # Boltzmann constant in J/K\n",
        "    T     = 300\n",
        "    gamma = 1e10\n",
        "    \n",
        "    file = open(\"Output.dat\", \"w+\")\n",
        "    \n",
        "    Stepnumber = 2000 \n",
        "    dt  = 10.**(-15)   # in s\n",
        "    dt2 = dt*dt\n",
        "    dtsq = np.sqrt(dt)\n",
        "    angle = 67        # in degrees\n",
        "    Ekin = 0.55       # in eV\n",
        "    \n",
        "    Integrator = 0\n",
        "    \n",
        "    print (\"Hallo\")\n",
        "    # Integratorin wird oben definiert\n",
        "    #Integratorin = input (\"Integrator=0-Verlet 1-VelocityVerlet 2-Langevin: 3-Monte Carlo: \")\n",
        "    IntegratorinList = [\"Verlet\",\"VelocityVerlet\",\"Langevin\",\"Monte Carlo\"]\n",
        "    print(f'Integrator ist {IntegratorinList[Integratorin]} \\n')\n",
        "    print(f'Temperatur ist {Tempin} K')\n",
        "    Integrator=int(Integratorin)\n",
        "    if Integrator==2:\n",
        "        # Tempin auch aus Funktion\n",
        "        #Tempin = input (\"Simulationstemperatur in K: \")\n",
        "        T=float(Tempin)\n",
        "        print (T)\n",
        "        # Gammain auch aus Funktion\n",
        "        #Gammain = input (\"Langevin Reibungskoeffizient gamma in 10^x /s (Vorschlag: 10^13/s): \")\n",
        "        gamma=10**int(Gammain)\n",
        "        sigma=np.sqrt(2*kB*T*gamma/m)\n",
        "        print (gamma)\n",
        "        \n",
        "    if Integrator==3:\n",
        "        # Tempin = input (\"Simulationstemperatur in K (fuehrt nur zur Skalierung der Zeitachse): \")\n",
        "        T=float(Tempin)\n",
        "        print (T)    \n",
        "        # Gammain = input (\"Vorfaktor fuer Ratenkonstante in 10^x /s (Vorschlag: 10^13/s) : \")\n",
        "        gamma=10**int(Gammain)\n",
        "        rate=gamma*np.exp(-0.5*V0/(kB*T))\n",
        "        print (\"Sprungrate bei T = \",T,\" K: \",rate,\" /s\")\n",
        "        \n",
        "    # Stepnumberin auch aus Funktion\n",
        "    # Stepnumberin = input (\"Anzahl der Schritte: \")\n",
        "    print(f'Schritte sind {Stepnumberin}')\n",
        "    Stepnumber=int(Stepnumberin)\n",
        "    print (Stepnumber)\n",
        "    if Integrator !=3 :\n",
        "        # Ekinin auch aus Funktion\n",
        "        # Ekinin = input (\"Kinetische Energie in eV: \")\n",
        "        Ekin=float(Ekinin)/1000\n",
        "        print (Ekin)\n",
        "        # anlein auch aus Funktion\n",
        "        # anglein = input (\"Anfangsrichtung in Grad: \")\n",
        "        angle=float(anglein)\n",
        "        print (angle)\n",
        "    \n",
        "    \n",
        "    [Positionsa, Etota, Ekina, vlast, R2, R2D] = Initialize(Ekin*eVtoJ, float(angle)/180.*np.pi, Stepnumber, dt) # Nullvektoren werden erstellt mit Länge=Stepnumber. Erste und zweite Dimension werden berechnet\n",
        "    \n",
        "    if Integrator==0 :\n",
        "        for i in range(Stepnumber):\n",
        "            if i>1: #Pos 0 und 1 überspringen, die wurden bereits von Initialize berechnet\n",
        "                Posout, Enerout, Ekinout = Verlet(Positionsa[:,i-1],Positionsa[:,i-2], dt)  # Posout: Position an diesem step (i) | Ekinout: Ekin am letzten Step (i-1)\n",
        "                Positionsa[:,i] = Posout\n",
        "                R2D[i]=(((Positionsa[0,i]-Positionsa[0,1])*10**10)**2 +  ((Positionsa[1,i]-Positionsa[1,1])*10**10)**2) \n",
        "                Ekina[i-1] = Ekinout\n",
        "                Etota[i-1] = Enerout+Ekinout\n",
        "                file.write (\"%i %8.5f %8.5f %8.5f\\n\" %  (i , Enerout/eVtoJ, Ekinout/eVtoJ , Etota[i-1]/eVtoJ)) \n",
        "    elif Integrator==1 :\n",
        "        for i in range(Stepnumber):\n",
        "            if i>0: #Pos 0 überspringen, die wurden bereits von Initialize berechnet\n",
        "                Posout, vlast, Enerout, Ekinout = VelVerlet(Positionsa[:,i-1], vlast, dt)\n",
        "                Positionsa[:,i] = Posout\n",
        "                R2D[i]=(((Positionsa[0,i]-Positionsa[0,1])*10**10)**2 +  ((Positionsa[1,i]-Positionsa[1,1])*10**10)**2) \n",
        "                Ekina[i] = Ekinout\n",
        "                Etota[i] = Enerout+Ekinout\n",
        "                file.write (\"%i %8.5f %8.5f %8.5f %9.5f\\n\" %  (i , Enerout/eVtoJ, Ekina[i]/eVtoJ , Etota[i]/eVtoJ, R2D[i])) \n",
        "    elif Integrator==2 :\n",
        "        for i in range(Stepnumber):\n",
        "            if i>0: #Pos 0 überspringen, die wurden bereits von Initialize berechnet\n",
        "                rand1=np.random.randn(2)\n",
        "                rand2=np.random.randn(2)\n",
        "                Posout, vlast, Enerout, Ekinout = Langevin(Positionsa[:,i-1], vlast, dt, rand1, rand2)\n",
        "                Positionsa[:,i] = Posout\n",
        "                R2D[i]=(((Positionsa[0,i]-Positionsa[0,1])*10**10)**2 +  ((Positionsa[1,i]-Positionsa[1,1])*10**10)**2) \n",
        "                Ekina[i] = Ekinout\n",
        "                Etota[i] = Enerout+Ekinout\n",
        "                file.write (\"%i %8.5f %8.5f %8.5f %9.5f\\n\" %  (i , Enerout/eVtoJ, Ekina[i]/eVtoJ , Etota[i]/eVtoJ, R2D[i])) \n",
        "    \n",
        "    elif Integrator==3 :\n",
        "        for i in range(Stepnumber):\n",
        "            if i>0: #Pos 0 überspringen, die wurden bereits von Initialize berechnet\n",
        "                Positionsa[:,i]=Positionsa[:,(i-1)] \n",
        "                rand3=random.randint(1,4)\n",
        "                if rand3==1:\n",
        "                   Positionsa[0,i]=Positionsa[0,(i-1)] + 3.0*10**(-10)\n",
        "                elif rand3==2:\n",
        "                   Positionsa[0,i]=Positionsa[0,(i-1)] - 3.0*10**(-10)\n",
        "                elif rand3==3:\n",
        "                   Positionsa[1,i]=Positionsa[1,(i-1)] + 3.0*10**(-10)\n",
        "                elif rand3==4:\n",
        "                   Positionsa[1,i]=Positionsa[1,(i-1)] - 3.0*10**(-10)\n",
        "    \n",
        "                   \n",
        "            R2D[i]=(((Positionsa[0,i]-Positionsa[0,1])*10**10)**2 +  ((Positionsa[1,i]-Positionsa[1,1])*10**10)**2) \n",
        "            file.write (\"%i %9.5f\\n\" %  (i , R2D[i])) \n",
        "    \n",
        "    # Berechnung des Abstandsquadrates als Funktion von t durch Mittelung über unterschiedliche Startpunkte, dadurch nur die halbe Anzahl der Schritte            \n",
        "    Steprange=int(Stepnumber/2)\n",
        "    # Alle Stepstart Zeitschritte wird ein neue Bestimmung von r^2 gestartet\n",
        "    Stepstart=200\n",
        "    Mittelung=int(Steprange/Stepstart)\n",
        "    \n",
        "    \n",
        "    print (Stepnumber, Steprange, Mittelung)\n",
        "    \n",
        "    if Mittelung>0:\n",
        "        print (\"Summieren\")\n",
        "        for i in range(1,Steprange):\n",
        "            for j in range(1,Mittelung):\n",
        "                l=(j-1)*Stepstart\n",
        "                k=i+l\n",
        "                R2[i]=R2[i]+ (((Positionsa[0,k]-Positionsa[0,l+1])*10**10)**2 +  ((Positionsa[1,k]-Positionsa[1,l+1])*10**10)**2) \n",
        "    elif Mittelung==0 :\n",
        "        print (\"Schrittanzahl zu gering zur Berechnung des mittleren Abstandsquadrats\")\n",
        "    \n",
        "    # Teilen durch die Anzahl der unterschiedlichen Startpunkte    \n",
        "    for i in range(Steprange):\n",
        "        R2[i]=R2[i]/Mittelung\n",
        "        R2wert=R2[i]*10**(-20) \n",
        "    # Diffusionskonstante berechnet aus der Rate: D = rate*a^2/4 \n",
        "    if Integrator==3 :\n",
        "        print (\"Einstein-Diffusionskonstante = \", (rate*(3.*10**(-10))**2)/4, \"m^2/s \" )\n",
        "    # Diffusionskonstante abgeschätzt aus dem Endwert der Trajektorie, wobei die Zeit reskaliert werden muss\n",
        "        Diffconstant=R2wert/(4*float((Steprange/rate)/dt)*10**(-15))\n",
        "        print (\"Diffusionskonstante abgeschaetzt aus Endpunkt der Simulation = \", Diffconstant , \"m^2/s \" )\n",
        "    if Integrator==2 :\n",
        "    # Diffusionskonstante abgeschätzt aus dem GLE Trajektorie\n",
        "        Diffconstant=R2wert/(4*float(Steprange*10**(-15)))\n",
        "        print (\"Diffusionskonstante abgeschaetzt aus Endpunkt der Simulation = \", Diffconstant , \"m^2/s \" )\n",
        "    \n",
        "    \n",
        "    \n",
        "    \n",
        "    ################################################################\n",
        "    #now this is just for the underlying potential contourplot\n",
        "    \n",
        "    minx = np.min(Positionsa[0,:])\n",
        "    maxx = np.max(Positionsa[0,:])\n",
        "    miny = np.min(Positionsa[1,:])\n",
        "    maxy = np.max(Positionsa[1,:])\n",
        "    \n",
        "    x = np.linspace(minx-10**-10, maxx+10**-10, 300)\n",
        "    y = np.linspace(miny-10**-10, maxy+10**-10, 300)\n",
        "    \n",
        "    xv, yv = np.meshgrid(x, y)\n",
        "    z = Potential(np.array([xv, yv]))[0]\n",
        "    \n",
        "    # Create 2x2 subplot grid\n",
        "    fig, axs = plt.subplots(2, 2, figsize=(15, 8))\n",
        "    fig.subplots_adjust(wspace=0.5, hspace=0.5)\n",
        "    \n",
        "    ##################################################################\n",
        "    # Plot 1: Contour plot with trajectory (top-left)\n",
        "    axs[0,0].contourf(xv*10**10, yv*10**10, z)\n",
        "    axs[0,0].plot(Positionsa[0,:]*10**10, Positionsa[1,:]*10**10, lw=5, c=\"k\", alpha=0.7)\n",
        "    axs[0,0].set_xlabel('x in Angstrom')\n",
        "    axs[0,0].set_ylabel('y in Angstrom')\n",
        "    if Integrator==3:\n",
        "        axs[0,0].set_title(\"MC: Atom Diffusion 2D Random Walk\")\n",
        "    else:\n",
        "        axs[0,0].set_title(\"MD: Atom Diffusion auf einer Oberflaeche\")\n",
        "    \n",
        "    # Plot 2: Energy vs time (top-right)\n",
        "    if Integrator != 3:\n",
        "        axs[0,1].set_xlabel('Zeitschritt (fs)')\n",
        "        axs[0,1].set_ylabel('Energie (eV)')\n",
        "        axs[0,1].set_xlim([0, (Stepnumber-5)])\n",
        "        axs[0,1].set_ylim([0, (Ekin+0.2)])\n",
        "        \n",
        "        j = np.arange(0, Stepnumber, 1)\n",
        "        axs[0,1].plot(j, Etota[:]/(1.6022*10**(-19)), color=\"red\", linewidth=2.5, \n",
        "                      linestyle=\"-\", label='Gesamtenergie')\n",
        "        axs[0,1].plot(j, Ekina[:]/(1.6022*10**(-19)), color=\"blue\", linewidth=2.5, \n",
        "                      linestyle=\"-\", label='Kinetische Energie')\n",
        "        axs[0,1].legend(loc='upper left', frameon=True)\n",
        "        axs[0,1].set_title(\"Energy vs Time\")\n",
        "    else:\n",
        "        axs[0,1].text(0.5, 0.5, 'No energy plot\\nfor Monte Carlo', \n",
        "                      ha='center', va='center', transform=axs[0,1].transAxes)\n",
        "        axs[0,1].set_title(\"Energy (N/A for MC)\")\n",
        "    \n",
        "    # Plot 3: Distance squared from origin (bottom-left)\n",
        "    if Integrator == 3:\n",
        "        axs[1,0].set_xlabel('Zeit (ps)')\n",
        "    else:\n",
        "        axs[1,0].set_xlabel('Zeit (fs)')\n",
        "    axs[1,0].set_ylabel('r2 (A2)')\n",
        "    \n",
        "    j = np.arange(0, Stepnumber, 1)\n",
        "    if Integrator == 3:\n",
        "        axs[1,0].plot((j/rate)/(dt*1000.), R2D[:], color=\"red\", linewidth=2.5, \n",
        "                      linestyle=\"-\", label='Abstandsquadrat bei der Ursprungstrajektorie')\n",
        "    else:\n",
        "        axs[1,0].plot(j, R2D[:], color=\"red\", linewidth=2.5, \n",
        "                      linestyle=\"-\", label='Abstandsquadrat bei der Ursprungstrajektorie')\n",
        "    axs[1,0].legend(loc='upper left', frameon=True)\n",
        "    axs[1,0].set_title(\"Distance² from Origin\")\n",
        "    \n",
        "    # Plot 4: Averaged distance squared (bottom-right)\n",
        "    if Integrator == 3:\n",
        "        axs[1,1].set_xlabel('Zeit (ps)')\n",
        "    else:\n",
        "        axs[1,1].set_xlabel('Zeit (fs)')\n",
        "    axs[1,1].set_ylabel('r2 (A2)')\n",
        "    \n",
        "    j = np.arange(0, Steprange, 1)\n",
        "    if Integrator == 3:\n",
        "        axs[1,1].plot((j/rate)/(dt*1000.), R2[:Steprange], color=\"red\", linewidth=2.5, \n",
        "                      linestyle=\"-\", label='MC: Gemitteltes Abstandsquadrat')\n",
        "    else:\n",
        "        axs[1,1].plot(j, R2[:Steprange], color=\"red\", linewidth=2.5, \n",
        "                      linestyle=\"-\", label='MD: Gemitteltes Abstandsquadrat')\n",
        "    axs[1,1].legend(loc='upper left', frameon=True)\n",
        "    axs[1,1].set_title(\"Averaged Distance²\")\n",
        "    \n",
        "    plt.show()\n",
        "    return"
      ]
    },
